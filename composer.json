{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-pdo_sqlite": "*",
        "ext-redis": "*",
        "bhcosta90/laravel-package": "^2.0",
        "bhcosta90/laravel-table": "^1.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "kris/laravel-form-builder": "1.40.0",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "nwidart/laravel-modules": "^9.0",
        "stancl/tenancy": "^3.5",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.13",
        "laravel/telescope": "^4.7",
        "lucascudo/laravel-pt-br-localization": "^1.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "overtrue/phplint": "^4.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "migrate": [
            "@php artisan migrate --force"
        ],
        "migrate:fresh": [
            "@php artisan migrate:fresh --force"
        ],
        "migrate:fresh-seed": [
            "composer run migrate:fresh",
            "@php artisan db:seed --force"
        ],
        "costa": [
            "@php artisan db:seed --class='Database\\Seeders\\CostaSeeder' --force"
        ],
        "migrate:costa": [
            "composer run migrate:fresh",
            "composer run costa"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
